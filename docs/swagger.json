        "/api/kyc/request": {
            "get": {
                "description": "Returns the number of KYC requests with a specific status (e.g., submitted) for the given date range (day, week, month)",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "KYC Requests"
                ],
                "summary": "Get KYC requests",
                "parameters": [
                    {
                        "type": "integer",
